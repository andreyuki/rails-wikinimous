<div class="container">
  <h1>Articles</h1>
  <%= link_to "New Article", new_article_path %>
  <% @articles.each do |article| %>
    <div class="article">
      <%= link_to article_path(article) do %>
        <h2><%= article.title %></h2>
      <% end %>
      <p><%= article.content %></p>
    </div>
  <% end %>
</div>
